{
    "name": "transloadit/php-sdk",
    "type": "library",
    "description": "Transloadit SDK",
    "keywords": [
        "transloadit",
        "video",
        "encoding",
        "thumbnails",
        "image",
        "resizing",
        "audio",
        "waveform",
        "document",
        "processing",
        "file",
        "uploading",
        "php",
        "sdk"
    ],
    "homepage": "https://github.com/transloadit/php-sdk",
    "license": "MIT",
    "version": "3.0.4",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "symfony/property-access": "^4.4 || ^5.1",
        "symfony/serializer": "^4.4 || ^5.1",
        "doctrine/annotations": "^1.11",
        "symfony/http-client": "^4.4 || ^5.1",
        "symfony/mime": "^4.4 || ^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8 || ^9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Transloadit\\": "src/",
            "transloadit\\": "lib/transloadit"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Transloadit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-clover coverage.clover --testdox",
        "phpcs": "vendor/bin/phpcs"
    }
}
